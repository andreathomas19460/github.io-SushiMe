{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true,
      "jquery": true,
      "jest": true,
      "mocha": true
    },
   
    "extends": ["eslint:recommended"],
    "rules": {
      "backtick": "error",
      "camelcase": "error",
      "eqeqeq": "error",
      "no-console": "off",
      "no-unused-vars": ["error"],
      "prefer-const": "error"
    }
  }
  